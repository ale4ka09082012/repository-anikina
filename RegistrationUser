// Generated by Selenium IDE
using System;
using System.Collections;
using System.Collections.Generic;
using System.Linq;
using System.Threading;
using OpenQA.Selenium;
using OpenQA.Selenium.Chrome;
using OpenQA.Selenium.Firefox;
using OpenQA.Selenium.Remote;
using OpenQA.Selenium.Support.UI;
using OpenQA.Selenium.Interactions;
using NUnit.Framework;
[TestFixture]
public class RegistrationUserTest {
  private IWebDriver driver;
  public IDictionary<string, object> vars {get; private set;}
  private IJavaScriptExecutor js;
  [SetUp]
  public void SetUp() {
    driver = new ChromeDriver();
    js = (IJavaScriptExecutor)driver;
    vars = new Dictionary<string, object>();
  }
  [TearDown]
  protected void TearDown() {
    driver.Quit();
  }
  [Test]
  public void registrationUser() {
    driver.Navigate().GoToUrl("https://qauto2.forstudy.space/");
    driver.Manage().Window.Size = new System.Drawing.Size(1200, 645);
    driver.FindElement(By.XPath("//button[contains(.,\'Sign up\')]")).Click();
    driver.FindElement(By.XPath("//input[@id=\'signupName\']")).Click();
    driver.FindElement(By.XPath("//input[@id=\'signupName\']")).SendKeys("Vita");
    driver.FindElement(By.XPath("//input[@id=\'signupLastName\']")).Click();
    driver.FindElement(By.XPath("//input[@id=\'signupLastName\']")).SendKeys("Ivanova");
    driver.FindElement(By.XPath("//input[@id=\'signupLastName\']")).SendKeys(Keys.Enter);
    driver.FindElement(By.XPath("//input[@id=\'signupEmail\']")).Click();
    driver.FindElement(By.XPath("//input[@id=\'signupEmail\']")).SendKeys("Vita01234@gmail.com");
    driver.FindElement(By.XPath("//input[@id=\'signupPassword\']")).Click();
    driver.FindElement(By.XPath("//input[@id=\'signupPassword\']")).SendKeys("Vita123456");
    driver.FindElement(By.XPath("//form/div[5]")).Click();
    driver.FindElement(By.XPath("//input[@id=\'signupRepeatPassword\']")).Click();
    driver.FindElement(By.XPath("//input[@id=\'signupRepeatPassword\']")).SendKeys("Vita123456");
    driver.FindElement(By.XPath("//button[contains(.,\'Register\')]")).Click();
    {
      WebDriverWait wait = new WebDriverWait(driver, System.TimeSpan.FromSeconds(10));
      wait.Until(driver => driver.FindElements(By.XPath("//*[@id=\"userNavDropdown\"]")).Count > 0);
    }
    var elements = driver.FindElements(By.XPath("//*[@id=\"userNavDropdown\"]"));
    Assert.True(elements.Count > 0);
  }
}
